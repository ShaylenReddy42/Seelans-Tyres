{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    }
  },
  "LoggingSinks": {
    "Elasticsearch": {
      "Enabled": "$(envUseElasticsearch ?? false)",
      "Url": "$(envElasticsearchUrl ?? http://localhost:6001)"
    }
  },
  "AllowedHosts": "*",
  "SqlServerHost": "$(envSqlServerHost ?? localhost)",
  "SeelansTyresOrderContext": "Server=$(SqlServerHost);Initial Catalog=SeelansTyresOrderDb;Trusted_Connection=true;MultipleActiveResultSets=true",
  "TokenIssuer": "$(envTokenIssuer ?? https://localhost:5005)"
}
